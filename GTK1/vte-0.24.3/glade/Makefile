install-catalogueDATA: $(catalogue_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(cataloguedir)" || $(MKDIR_P) "$(DESTDIR)$(cataloguedir)"
	@list='$(catalogue_DATA)'; test -n "$(cataloguedir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(cataloguedir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(cataloguedir)" || exit $$?; \
	done

uninstall-catalogueDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(catalogue_DATA)'; test -n "$(cataloguedir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(cataloguedir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(cataloguedir)" && rm -f $$files
